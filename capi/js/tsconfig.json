{
    "compilerOptions": {
        "outDir": "./node/",
        "sourceMap": false,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "alwaysStrict": true,
        "strictFunctionTypes": true,
        "strict": true,
        "declaration": true
    },
    "include": [
        "../bindings/emscripten/livesplit_core.ts"
    ]
}
